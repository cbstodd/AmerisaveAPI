{
  "name": "ts-api-server",
  "version": "0.0.1",
  "description": "Basic TypeScript/Node, ExpressJS, and MongoDB app with login and messaging dashboard",
  "main": "dist/app.js",
  "scripts": {
    "build": "tsc src/app.ts --outDir dist/",
    "start": "npm run build && nodemon --unhandled-rejections=strict ./dist/app.js",
