  },
  "homepage": "https://github.com/cbstodd/tsRESTapi#readme",
  "dependencies": {
    "cors": "^2.8.5",
    "debug": "^4.3.2",
    "express": "^4.20.0",
    "express-winston": "^4.2.0",
    "shortid": "^2.2.16",
    "winston": "^3.3.3"
  },
  "devDependencies": {
    "@types/cors": "^2.8.12",
    "@types/debug": "^4.1.7",
    "@types/express": "^4.17.13",
